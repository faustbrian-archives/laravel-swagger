{
    "name": "faustbrian/laravel-swagger",
    "description": "Swagger UI Integration for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Swagger", "Laravel Swagger", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.* || 5.6.*",
        "illuminate/support": "5.5.* || 5.6.*",
        "doctrine/dbal": "^2.5",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Swagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Swagger\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Swagger\\SwaggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
